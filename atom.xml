---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Alberto Grespan</title>
 <link href="http://albertogrespan.com/atom.xml"/>
 <link href="http://albertogrespan.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://albertogrespan.com/</id>
 <author>
   <name>Alberto Grespan</name>
   <email>support@albertogrespan.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title | xml_escape }}</title>
   <link href="http://albertogrespan.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://albertogrespan.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
